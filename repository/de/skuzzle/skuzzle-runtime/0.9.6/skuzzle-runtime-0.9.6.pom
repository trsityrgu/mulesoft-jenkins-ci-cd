    <packaging>pom</packaging>
    <url>http://www.skuzzle.de/maven/</url>

    <name>skuzzle-runtime</name>
    <description>Skuzzle Dependency Declarations</description>
    <inceptionYear>2015</inceptionYear>

    <properties>
